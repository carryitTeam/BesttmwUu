<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.carryit.base.besttmwuu.dao.MessageCodeDao">


    <select id="getIdByPhone" resultType="com.carryit.base.besttmwuu.entity.MessageCode" parameterType="Integer">
        select * FROM ims_message WHERE phone=#{phoneNumber}
    </select>

    <insert id="insert">
        insert into Author (id,phone,code,createTime,status)
        values (#{id},#{phoneNumber},#{code},#{createTime},#{status})
    </insert>

    <update id="update">
        update Author set
        id = #{id},
        phone = #{phoneNumber},
        code = #{code},
        status = #{status}
        where phone = #{phoneNumber}
    </update>
</mapper>